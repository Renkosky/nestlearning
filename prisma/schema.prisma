// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int       @id @unique @default(autoincrement())
  email     String
  name      String?
  Project   Project[]
  projectId Int?
}

model Project {
  id      Int     @id @unique @default(autoincrement())
  name    String
  owner   User[]
  cover   String?
  devUrl  String?
  uatUrl  String?
  prodUrl String?
}
